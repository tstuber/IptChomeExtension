{
  "background": {
    "page": "background.js",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "static/icon19.png",
    "default_popup": "browser_action.html",
    "default_title": "ipt Extension"
  },
  "content_scripts": [{
    "all_frames": true,
    "js": ["dict-cc-contentscript.js"],
    "matches": ["\u003Call_urls>"]
  }],
  "icons": {
    "128": "static/icon128.png",
    "16": "static/icon16.png",
    "32": "static/icon32.png",
    "48": "static/icon48.png"
  },
  "name": "Identify ipt Employees",
  "description": "Select an Abbriviation on a page and show who it is.",
  "permissions": ["storage", "tabs"],
  "version": "0.1",
  "manifest_version": 2,
"web_accessible_resources": [ "content.min.css" ]
}
